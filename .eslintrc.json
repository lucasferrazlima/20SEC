{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "camelcase": "off",
        "no-unused-vars": "off",
        "no-undef": "off"
    }
}
